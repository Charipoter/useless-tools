<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="231.0" prefWidth="402.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.useless.core.discard.system.LoginController">
   <children>
      <VBox alignment="CENTER" layoutX="117.0" prefHeight="231.0" prefWidth="402.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="30.0" prefWidth="402.0" spacing="30.0">
               <children>
                  <Label prefHeight="38.0" prefWidth="54.0" text="账号">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Label>
                  <TextField fx:id="accountField" prefHeight="30.0" prefWidth="248.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="35.0" prefWidth="402.0" spacing="30.0">
               <children>
                  <Label prefHeight="38.0" prefWidth="54.0" text="密码">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Label>
                  <TextField fx:id="passwordField" prefHeight="30.0" prefWidth="248.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="46.0" prefWidth="394.0">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#tryLogin" text="登录">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Button>
                  <Label fx:id="loginMessage" alignment="CENTER" prefHeight="40.0" prefWidth="192.0" textFill="#2191b4">
                     <font>
                        <Font size="23.0" />
                     </font>
                  </Label>
                  <Button mnemonicParsing="false" onMouseClicked="#register" text="注册">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
